<UserControl x:Class="Prover.GUI.Modules.Certificates.Common.ValueLabelControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Prover.GUI.Modules.Certificates.Common"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    
    
    <StackPanel Orientation="Vertical">
        <TextBlock Text="{Binding Pressure.TransducerType, FallbackValue=[N]}"
                   Style="{StaticResource ValueStyle}" />
        <TextBlock Text="transducer"
                   Style="{StaticResource LabelStyle}" />
    </StackPanel>
</UserControl>
